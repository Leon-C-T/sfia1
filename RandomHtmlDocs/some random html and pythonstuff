cur = mysql.connection.cursor()
            cur.execute("SELECT Destination.Name, Destination.Address FROM Destination JOIN DesJoin ON Destination.Des_ID = DesJoin.Destination_ID WHERE DesJoin.Trip_ID = %s",[tripidval])
            destinationinfo = cur.fetchall()
            print(destinationinfo)
            destinationinfo1 = destinationinfo[0]
            print(destinationinfo1)
            des1list = list(destinationinfo1)
            destinationinfo2 = destinationinfo[1]
            des2list = list(destinationinfo2)
            cur.execute("SELECT Restaurants.Name, Restaurants.Address, Restaurants.Type, Restaurants.Price FROM Restaurants JOIN Daytrip on Daytrip.Trip_Res_ID = Restaurants.Res_ID WHERE Daytrip.Trip_Res_ID = (%s)", [resid])
            resinfo = cur.fetchall()
            lisss = list(resinfo[0])
            print(lisss)
            session['res'] = lisss
            session['firName'] = lisss[1]
            session['Add'] = lisss[2]
            session['Type'] = lisss[3]
            print(lisss)
            cur.close()



